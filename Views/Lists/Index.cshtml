@model IEnumerable<dynamic>

<h1>Ranked Lists</h1>
@foreach (var list in Model)
{
    <div class="card mb-3">
        <div class="card-header">
            <h5>@list.Name</h5>
            <p>By @list.Creator</p>
        </div>
        <div class="card-body">
            <ul>
                @foreach (var album in list.Albums)
                {
                    <li>@album</li>
                }
            </ul>
        </div>
    </div>
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
